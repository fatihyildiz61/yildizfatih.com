    </noscript>
    

    
    <!-- Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-YCD1Z7NNPZ"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());
      gtag('config', 'G-YCD1Z7NNPZ', {
        page_title: document.title,
        page_location: window.location.href,
        custom_map: {
          'custom_dimension1': 'page_type',
          'custom_dimension2': 'service_category'
        }
      });
      
      // Enhanced Analytics Events
      document.addEventListener('DOMContentLoaded', function() {
        // Track page views
        gtag('event', 'page_view', {
          page_title: document.title,
          page_location: window.location.href
        });
        
        // Track form submissions
        const forms = document.querySelectorAll('form');
        forms.forEach(form => {
          form.addEventListener('submit', function() {
            gtag('event', 'form_submit', {
              form_name: form.getAttribute('action') || 'contact_form',
              page_location: window.location.href
            });
          });
        });
        
        // Track outbound links
        const links = document.querySelectorAll('a[href^="http"]');
        links.forEach(link => {
          link.addEventListener('click', function() {
            gtag('event', 'click_external_link', {
              link_url: link.href,
              link_text: link.textContent
            });
          });
        });
      });
    </script>
    
    <!-- Performance Monitoring -->
    <script>
      // Core Web Vitals monitoring
      if ('PerformanceObserver' in window) {
        const observer = new PerformanceObserver((list) => {
          list.getEntries().forEach((entry) => {
            if (entry.name === 'LCP') {
              gtag('event', 'web_vitals', {
                metric_name: 'LCP',
                metric_value: entry.startTime,
                metric_id: entry.id
              });
            }
            if (entry.name === 'FID') {
              gtag('event', 'web_vitals', {
                metric_name: 'FID',
                metric_value: entry.processingStart - entry.startTime,
                metric_id: entry.id
              });
            }
            if (entry.name === 'CLS') {
              gtag('event', 'web_vitals', {
                metric_name: 'CLS',
                metric_value: entry.value,
                metric_id: entry.id
              });
            }
          });
        });
        
        observer.observe({ entryTypes: ['largest-contentful-paint', 'first-input', 'layout-shift'] });
      }
    </script>
    
    <!-- ThemeProvider component -->
    <ThemeProvider />
    
    <!-- Critical CSS for initial render -->
    <style is:inline>
      /* Prevent FOUC (Flash of Unstyled Content) */
      html { 
        visibility: hidden; 
        font-display: swap;
      }
      html.theme-loaded { 
        visibility: visible; 
      }
      
      /* Critical CSS for initial render */
      body {
        font-family: system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif;
        margin: 0;
        padding: 0;
        line-height: 1.6;
      }
      
      /* Font loading optimization */
      .font-loaded {
        font-family: 'Inter', system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif;
      }
      
      /* Critical layout styles */
      .container {
        max-width: 1200px;
        margin: 0 auto;
        padding: 0 1rem;
      }
      
      /* Critical button styles */
      .btn {
        display: inline-flex;
        align-items: center;
        padding: 0.75rem 1.5rem;
        border-radius: 0.5rem;
        font-weight: 500;
        text-decoration: none;
        transition: all 0.2s ease;
      }
      
      /* Critical navigation styles */
      .nav {
        position: fixed;
        top: 0;
        width: 100%;
        z-index: 50;
        backdrop-filter: blur(8px);
      }
    </style>
    
    <script is:inline>
      // Enhanced theme initialization with performance optimizations
      (function() {
        try {
          // Set theme immediately to prevent flash
          var theme = localStorage.getItem('theme') || 
                      (window.matchMedia('(prefers-color-scheme: dark)').matches ? 'dark' : 'light');
          document.documentElement.classList.toggle('dark', theme === 'dark');
          
          // Add font loading detection
          if ('fonts' in document) {
            Promise.all([
              document.fonts.load('400 1em Inter'),
              document.fonts.load('500 1em Inter'),
              document.fonts.load('600 1em Inter'),
              document.fonts.load('700 1em Inter')
            ]).then(function() {
              document.documentElement.classList.add('font-loaded');
            });
          }
          
          // Show content after theme and fonts are ready
          document.documentElement.classList.add('theme-loaded');
        } catch (_) {
          // Fallback: show content immediately if there's an error
          document.documentElement.classList.add('theme-loaded');
        }
      })();
    </script>
  </head>
